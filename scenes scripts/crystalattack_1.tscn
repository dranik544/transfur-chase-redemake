[sub_resource type="GDScript" id="GDScript_o76m3"]
script/source = "extends Area3D


func _ready() -> void:
	rotation.y = randf_range(-90, 90)
	position += Vector3(
		randf_range(-0.1, 0.1),
		0,
		randf_range(-0.1, 0.1)
	)
	$Sprite3D.texture = load(\"res://sprites/greenbcrystal\" + str(randi_range(1, 3)) + \".png\")
	$Sprite3D2.texture = load(\"res://sprites/greenbcrystal\" + str(randi_range(1, 3)) + \".png\")
	
	body_entered.connect(bodyentered)

func bodyentered(body):
	if body.is_in_group(\"player\"):
		body.minushealth(randf_range(0.5, 1.0), Color.BLACK)

func toggledis(toggle: bool, randomize: bool = false):
	if randomize: toggle = randi_range(0, 1) == 0
	visible = toggle
	$CollisionShape3D.disabled = not toggle
"

