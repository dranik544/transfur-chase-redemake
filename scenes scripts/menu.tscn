[sub_resource type="GDScript" id="GDScript_jh7g4"]
script/source = "extends Node2D

@export var list = {
	1: preload(\"res://sprites/photo1.png\"),
	2: preload(\"res://sprites/photo2.png\"),
	3: preload(\"res://sprites/photo3.png\"),
	4: preload(\"res://sprites/photo4.png\"),
	5: preload(\"res://sprites/photo5.png\"),
}
@export var maxlist = 5
var defscale
var curscale
var defrotate
var currotate


func _ready() -> void:
	defscale = scale
	curscale = defscale
	defrotate = rotation
	currotate = defrotate
	
	$wiki.texture = list[randi_range(1, maxlist)]
	
	$Button.pressed.connect(btnpressed)
	$Button.mouse_entered.connect(btnmouseentered)
	$Button.mouse_exited.connect(btnmouseexited)

func _process(delta: float) -> void:
	scale = lerp(scale, curscale, 15 * delta)
	rotation = lerp(rotation, currotate, 15 * delta)

func btnpressed():
	print(\"pressed\")

func btnmouseentered():
	curscale = defscale + Vector2(0.1, 0.1)
	currotate = defrotate + deg_to_rad(5.0)
func btnmouseexited():
	curscale = defscale
	currotate = defrotate
"

