[sub_resource type="GDScript" id="GDScript_1w7ov"]
script/source = "extends Control


func falllogo2():
	var logopos = $logo2.position
	$logo2.visible = true
	$logo2.position.x = get_viewport().size.x / 2
	$logo2.position.y = 0.0 - $logo2.size.y - 25
	create_tween().tween_property($logo2, \"position:y\", logopos.y, 0.5)

func changescene():
	get_tree().change_scene_to_file(\"res://scenes scripts/animintro.tscn\")

func _unhandled_input(event: InputEvent) -> void:
	if Input.is_anything_pressed():
		ScreenTransition.changescene(\"res://scenes scripts/animintro.tscn\", Color.BLACK, 1.0)
"

