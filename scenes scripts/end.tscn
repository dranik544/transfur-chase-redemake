	create_tween().tween_property($TextureRect, \"modulate:a\", 0.0, 0.75)
	await get_tree().create_timer(0.75).timeout
	
	changescene()

func changescene():
	get_tree().change_scene_to_file(\"res://scenes scripts/menu.tscn\")
